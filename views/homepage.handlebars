<div>
    <h2>Homepage</h2>

    <ul>
        {{#each posts as |post|}}
        <a href="/post/{{post.id}}">
            <li>
                <div>
                    <h2>{{post.title}}</h2>
                    <p>{{post.text}}</p>
                </div>
            </li>
         </a>
        {{/each}}
    </ul>
    
</div>

